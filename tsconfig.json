{
    "include": [
        "src/**/*.ts"
    ],
    "compilerOptions": {
        "outDir": "dist",
        "strictNullChecks": true,
        "target": "ES2020",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "moduleResolution": "Node",
        "sourceMap": true,
        "declaration": true,
        "declarationDir": "dist",
    },
}
